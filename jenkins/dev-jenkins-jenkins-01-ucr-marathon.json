{
  "id": "/dev/jenkins/jenkins-01",
  "instances": 1,
  "cpus": 2,
  "mem": 4096,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "MESOS",
    "docker": {
      "image": "mesosphere/jenkins:3.5.0-2.107.2"
    },
    "volumes": [
      {
        "containerPath": "dev-jenkins-jenkins-01",
        "external": {
          "size": 10,
          "name": "dev-jenkins-jenkins-01",
          "provider": "dvdi",
          "options": {
            "dvdi/driver": "rexray"
          }
        },
        "mode": "RW"
      },
      {
        "containerPath": "/var/jenkins_home",
        "hostPath": "dev-jenkins-jenkins-01",
        "mode": "RW"
      }
    ]
  },
  "secrets": {
    "serviceCredential": {
      "source": "dev/jenkins/serviceCredential"
    }
  },
  "env": {
    "DCOS_SERVICE_ACCOUNT_CREDENTIAL": {
      "secret": "serviceCredential"
    },
    "JENKINS_AGENT_ROLE": "dev__jenkins__agent-role",
    "JENKINS_AGENT_USER": "nobody",
    "JENKINS_CONTEXT": "/service/dev/jenkins/jenkins-01",
    "JENKINS_FRAMEWORK_NAME": "/dev/jenkins/jenkins-01",
    "JENKINS_MESOS_MASTER": "zk://zk-1.zk:2181,zk-2.zk:2181,zk-3.zk:2181,zk-4.zk:2181,zk-5.zk:2181/mesos",
    "JENKINS_OPTS": "",
    "JENKINS_ROOT_URL": "https://vhost.example.com/service/dev/jenkins/jenkins-01",
    "JENKINS_SLAVE_AGENT_PORT": "0",
    "JVM_OPTS": "-Xms2048m -Xmx2048m",
    "MESOS_AUTHENTICATEE": "com_mesosphere_dcos_ClassicRPCAuthenticatee",
    "MESOS_MODULES": "{\"libraries\": [{\"file\": \"libdcos_security.so\", \"modules\": [{\"name\": \"com_mesosphere_dcos_ClassicRPCAuthenticatee\"}]}]}",
    "SSH_KNOWN_HOSTS": "github.com git.apache.org"
  },
  "labels": {
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "/dev/jenkins/jenkins-01",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_NAME": "jenkins",
    "DCOS_PACKAGE_OPTIONS": "",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_VERSION": "3.5.0-2.107.2",
    "DCOS_SERVICE_NAME": "/dev/jenkins/jenkins-01",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_SERVICE_SCHEME": "http",
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_ENABLED": "true",
    "HAPROXY_0_VHOST": "vhost.example.com",
    "HAPROXY_0_REDIRECT_TO_HTTPS": "true",
    "HAPROXY_0_PATH": "/service/dev/jenkins/jenkins-01",
    "HAPROXY_0_HTTP_BACKEND_PROXYPASS_PATH": "/service/dev/jenkins/jenkins-01"
  },
  "networks": [
    {
      "mode": "host"
    }
  ],
  "portDefinitions": [
    {
      "name": "nginx",
      "port": 0,
      "protocol": "tcp",
      "labels": {
        "VIP_0": "/dev/jenkins/jenkins-01:80"
      }
    },
    {
      "name": "jenkins",
      "port": 0,
      "protocol": "tcp",
      "labels": {
        "VIP_1": "/dev/jenkins/jenkins-01:8080"
      }
    }
  ],
  "healthChecks": [
    {
      "portIndex": 0,
      "protocol": "MESOS_HTTP",
      "path": "/service/dev/jenkins/jenkins-01/login",
      "gracePeriodSeconds": 180,
      "intervalSeconds": 20,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "unreachableStrategy": {
    "expungeAfterSeconds": 0,
    "inactiveAfterSeconds": 0
  }
}
