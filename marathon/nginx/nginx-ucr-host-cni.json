{
  "id": "/nginx-ucr-docker-host-cni",
  "instances": 2,
  "cpus": 0.01,
  "mem": 32,
  "cmd": "env | sort && nginx -g \"daemon off;\"",
  "container": {
    "type": "MESOS",
    "docker": {
      "image": "nginx:alpine",
      "network": "HOST"
    }
  },
  "ipAddress": {
    "networkName": "dcos",
    "discovery": {
      "ports": [
        {
          "number": 80,
          "name": "http",
          "protocol": "tcp"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "MESOS_HTTP",
      "path": "/",
      "port": 80,
      "gracePeriodSeconds": 20,
      "intervalSeconds": 20,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "labels": {
    "HAPROXY_0_VHOST": "<vhost.example.com>",
    "HAPROXY_GROUP": "external"
  }
}
