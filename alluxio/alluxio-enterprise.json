  "labels": {
    "DCOS_MIGRATION_API_PATH": "/v1/plan",
    "DCOS_MIGRATION_API_VERSION": "v1",
    "DCOS_PACKAGE_COMMAND": "eyJwaXAiOlsiaHR0cHM6Ly9hbGx1eGlvLWRjb3MuczMuYW1hem9uYXdzLmNvbS9yZWxlYXNlLzEuMC4xLTEuNC4wL2Jpbl93cmFwcGVyLTAuMC4xLXB5Mi5weTMtbm9uZS1hbnkud2hsIl19",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "alluxio-enterprise",
    "DCOS_PACKAGE_IS_FRAMEWORK": "true",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_NAME": "alluxio-enterprise",
    "DCOS_PACKAGE_REGISTRY_VERSION": "2.0",
    "DCOS_PACKAGE_RELEASE": "0",
