    "TASKCFG_ALL_ALLUXIO_LOGLEVEL": "",
    "UFS_ADDRESS": "hdfs://hdfs",
    "WORKER_COUNT": "1",
    "WORKER_CPUS": "1",
    "WORKER_DISK": "1000",
    "WORKER_DISK_TYPE": "ROOT",
    "WORKER_MEM": "2048",
    "WORKER_PLACEMENT": "hostname:UNIQUE"
