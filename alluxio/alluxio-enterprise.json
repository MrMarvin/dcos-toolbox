{
  "id": "alluxio-enterprise",
  "cpus": 1,
  "mem": 1230,
  "instances": 1,
  "uris": [
    "https://downloads.mesosphere.com/java/jre-8u112-linux-x64.tar.gz",
    "https://downloads.mesosphere.com/libmesos-bundle/libmesos-bundle-1.9-argus-1.1.x-2.tar.gz",
    "https://alluxio-dcos.s3.amazonaws.com/v1.5.0/bootstrap-dcos.sh",
