{
  "id": "/janitor-__NAME__",
  "cmd": "sed -i s/marathon_url,/url,/g /janitor.py && /janitor.py -v -r __ROLE__ -p __PRINCIPAL__ -z __ZNODE__ --auth_token=__TOKEN__",
  "cpus": 0.01,
  "mem": 64,
  "instances": 1,
  "container": {
    "docker": {
      "image": "mesosphere/janitor:latest",
      "network": "HOST"
    },
    "type": "DOCKER"
  }
}
